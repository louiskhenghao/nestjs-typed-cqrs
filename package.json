{
  "name": "nestjs-typed-cqrs",
  "description": "Typed CQRS for NestJs",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Louis Loo <louiskhenghao@gmail.com> (https://github.com/louiskhenghao)",
  "homepage": "https://github.com/louiskhenghao/nestjs-typed-cqrs#readme",
  "keywords": [
    "nestjs",
    "cqrs",
    "typescript",
    "utility"
  ],
  "private": false,
  "main": "dist/index.js",
  "source": "src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "concurrently \"tsc -w -p tsconfig.json\"",
    "clean": "rimraf dist",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint-fix": "eslint --fix . --ext .ts,.tsx",
    "prettier": "prettier --write .",
    "cz": "git-cz",
    "prepublish": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nestjs-architects/typed-cqrs": "^1.1.2-alpha.0",
    "@nestjs/cqrs": "^10.0.1",
    "@ptc-org/nestjs-query-core": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "typescript": "^5.1.6"
  }
}
